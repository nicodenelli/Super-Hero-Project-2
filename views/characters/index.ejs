<%- include('../partials/header') %>

<body>
  <h1>Characters</h1>
  <table id="List">
    <thead>
      <tr>
        <th>Character</th>
        <th>Age</th>
        <th>Power Level</th>
        <th>Weapon</th>
      </tr>
    </thead>
    <tbody>

        <% characters.forEach(c => { %> <!-- "flights" comes from controller -->
         <tr>
           <td><%= c.character %></td>
           <td><%= c.age %></td>
           <td><%= c.powerLevel %></td>
           <td><%= c.weapon %></td>
           <td><a href="/supers/<%= c._id %>">Characters Detail</a></td>
         </tr>
         <% }) %>
       
     </tbody>
    
  </table>
</body>
</html>